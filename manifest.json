{
    "manifest_version": 3,
    "name": "Tradutor de Inglês para Português",
    "version": "1.0",
    "description": "Uma extensão para traduzir texto do inglês para português.",
    "permissions": ["activeTab", "storage"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_icon": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
      }
    },
    "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  }
  